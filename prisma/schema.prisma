generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FiberProvider {
  id                                String   @id @default(cuid())
  frn                              String   
  providerId                       String   @map("provider_id")
  brandName                        String   @map("brand_name")
  locationId                       String   @map("location_id")
  technology                       Int      
  maxAdvertisedDownloadSpeed       Int      @map("max_advertised_download_speed")
  maxAdvertisedUploadSpeed         Int      @map("max_advertised_upload_speed")
  lowLatency                       Int      @map("low_latency")
  businessResidentialCode          String   @map("business_residential_code")
  stateUsps                        String   @map("state_usps")
  blockGeoid                       String   @map("block_geoid")
  h3Res8Id                         String   @map("h3_res8_id")
  createdAt                        DateTime @default(now()) @map("created_at")
  updatedAt                        DateTime @updatedAt @map("updated_at")

  @@index([blockGeoid])
  @@index([brandName])
  @@index([stateUsps])
  @@map("fiber_providers")
}
